{
    "name": "kel2",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^13.3.0",
        "@testing-library/user-event": "^13.5.0",
        "axios": "^0.27.2",
        "classnames": "^2.3.1",
        "prop-types": "^15.8.1",
        "react": "^18.1.0",
        "react-dom": "^18.1.0",
        "react-hook-form": "^7.31.3",
        "react-redux": "^8.0.2",
        "react-router-dom": "^6.3.0",
        "react-scripts": "5.0.1",
        "redux": "^4.2.0",
        "redux-thunk": "^2.4.1",
        "sass": "^1.52.3",
        "swiper": "^8.2.4",
        "web-vitals": "^2.1.4"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "lint:prettier": "prettier --ignore-path .gitignore --check .",
        "lint:eslint": "eslint src/**/*.js",
        "lint:scss": "npx stylelint \"**/*.scss\"",
        "lint": "yarn lint:prettier && yarn lint:eslint && yarn lint:scss",
        "validate": "npm run lint && npm run build",
        "pre-commit": "pretty-quick --staged --ignore-path=.eslintignore && lint-staged",
        "pre-push": "npm run validate",
        "prettier": "npx prettier --write .",
        "prepare": "husky install"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "lint-staged": {
        "*.{js,jsx}": "eslint --fix"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm js-lint, npm style-lint"
        }
    },
    "devDependencies": {
        "eslint": "^8.17.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.30.0",
        "husky": "^8.0.1",
        "lint-staged": "^13.0.1",
        "prettier": "^2.6.2",
        "pretty-quick": "^3.1.3",
        "stylelint": "^14.9.0",
        "stylelint-config-css-modules": "^4.1.0",
        "stylelint-config-sass-guidelines": "^9.0.1"
    }
}
